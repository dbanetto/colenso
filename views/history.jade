extends layout

block navbar
  +nav_item("/") Home
  +nav_item("/browse") Browse
  +nav_item("/search") Search

block content
    .container
        h1 Search History
        hr
        if history
            +list-group-custom()
            each item in history
                +list-group-item(item.url)
                    span  #{item.query}
        else
            h2 No search history
        br
        a.btn.btn-default(href=".") Back to Admin
